{
  "name": "redirector",
  "version": "1.0.0",
  "description": "URL redirection service used by seeleft.de magazine.",
  "main": "dist/src/main.js",
  "scripts": {
    "dev:copy-static-assets": "copyfiles static/images/*.png static/css/*.css static/js/*.js dist/",
    "dev:watch": "tsc-watch --onSuccess \"run-s dev:copy-static-assets start:debug\"",
    "dev:lint": "echo \"Linting not yet implemented.\"",
    "dev:build": "npm run dev:lint & tsc & npm run dev:copy-static-assets",
    "start:debug": "node . --debug",
    "start": "node ."
  },
  "keywords": [ ],
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/seeleft/redirector"
  },
  "author": "Daniel Riegler",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/exit-hook": "^2.2.1",
    "@types/express": "^4.17.0",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.6.8",
    "@types/request": "^2.48.2",
    "@types/winston": "^2.4.4",
    "copyfiles": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "exit-hook": "^2.2.0",
    "express": "^4.17.1",
    "handlebars": "^4.1.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.3.0-beta2",
    "randexp": "^0.5.3",
    "request": "^2.88.0",
    "toml": "^3.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  }
}
